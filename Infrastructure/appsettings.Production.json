{
  "Domain": "dev",
  "OpenIdConnectUrl": "https://fr1psl013820.misys.global.ad:8443/auth/realms/Finastra/.well-known/openid-configuration",
  "CertificatePath": "conf.d/misys_global_ad.pfx",

  "NumberOfOracleShortTaskExecutors": 3,
  "ShortTimeoutInSeconds": 60,

  "NumberOfOracleLongTaskExecutors": 1,
  "LongTimeoutInMinutes": 60,

  "NumberOfOracleDiskIntensiveTaskExecutors": 1,
  "VeryLongTimeoutInMinutes": 60,

  "NumberOfWorkingCopyWorkers": 10,

  "TemporaryWorkingCopyLifetimeInHours": 168.0,

  "Akka:LogLevel": "INFO",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./log/PDBOrchestrator-{{SERVER_INSTANCE}}-.txt",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}][{RequestId}]{LogSource} {Message:lj}{NewLine}{Exception}",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Http",
        "Args": {
          "requestUri": "http://localhost:8081"
        }
      }
    ]
  }
}
