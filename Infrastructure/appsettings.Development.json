{
  "NumberOfOracleShortTaskExecutors": 10,
  "NumberOfOracleLongTaskExecutors": 3,
  "NumberOfOracleDiskIntensiveTaskExecutors": 1,
  "GarbageCollectionDelayInHours": 0.017,

  "Akka:LogLevel": "DEBUG",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo:0": {
      "Name": "Console",
      "Args": {
        "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}][{RequestId}]{LogSource} {Message:lj}{NewLine}{Exception}"
      }
    }
  }
}
