{
  "Akka:LogLevel": "DEBUG",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}][{RequestId}]{LogSource} {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "GarbageCollectionDelayInHours": 0.017
}
