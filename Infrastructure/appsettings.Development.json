{
  "CertificatePath": "misys_global_ad.pfx",

  "TemporaryWorkingCopyLifetimeInHours": 0.017,

  "Akka:LogLevel": "DEBUG",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo:0": {
      "Name": "Console",
      "Args": {
        "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}][{RequestId}]{LogSource} {Message:lj}{NewLine}{Exception}"
      }
    }
  }
}
